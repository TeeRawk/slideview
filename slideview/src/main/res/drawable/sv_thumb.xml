<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/buttonBackground">
        <shape
            android:shape="rectangle">
<size android:width="76dp" android:height="50dp"/>
            <solid
                android:color="#fff">
            </solid>


            <corners
                android:radius="5dp">
            </corners>
        </shape>
    </item>
    <item
        android:left="15dp"
        android:right="15dp"
        android:gravity="center"
        android:id="@+id/buttonImage"
        android:drawable="@drawable/sv_ic_chevron_double_right" />
</layer-list>
